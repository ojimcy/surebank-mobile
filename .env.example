# Environment Variables Template for Surebank Stores Mobile App
# Copy this file to .env and fill in your actual values
# Note: In Expo, environment variables must be prefixed with EXPO_PUBLIC_ to be available in the client

# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your-firebase-api-key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
EXPO_PUBLIC_FIREBASE_APP_ID=your-app-id

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://api.surebankstores.com
EXPO_PUBLIC_WS_BASE_URL=wss://ws.surebankstores.com
EXPO_PUBLIC_USE_FIREBASE_EMULATOR=false

# App Configuration
EXPO_PUBLIC_APP_NAME=Surebank Stores
EXPO_PUBLIC_APP_VERSION=1.0.0

# Environment
NODE_ENV=development
EXPO_PUBLIC_APP_ENV=development

# Feature Flags
EXPO_PUBLIC_ENABLE_MOCK_DATA=false
EXPO_PUBLIC_ENABLE_WEBSOCKETS=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true

# Debug
EXPO_PUBLIC_DEBUG_API_CALLS=true
EXPO_PUBLIC_DEBUG_WEBSOCKETS=false